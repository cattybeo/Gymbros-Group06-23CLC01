{
  "name": "gymbros",
  "main": "expo-router/entry",
  "version": "1.5.0",
  "scripts": {
    "start": "expo start",
    "android": "yarn setup:android && expo run:android",
    "ios": "yarn setup:ios && expo run:ios",
    "setup:ios": "test -d ios || yarn prebuild --platform ios; yarn ensure-credentials",
    "setup:android": "test -d android || yarn prebuild --platform android; yarn ensure-credentials",
    "prebuild": "expo prebuild",
    "postprebuild": "yarn ensure-credentials",
    "ensure-credentials": "mkdir -p ios/Gymbros android/app && cp credentials/GoogleService-Info.plist ios/Gymbros/ && cp credentials/google-services.json android/app/ && echo 'Credentials copied successfully'",
    "web": "expo start --web",
    "analysis:knip": "knip --reporter json > artifacts/knip-report.json || exit 0",
    "analysis:graph": "depcruise app components lib hooks --include-only \"^app|^components|^lib|^hooks\" --config .dependency-cruiser.js --output-type json > artifacts/depgraph.json",
    "analysis:context": "node scripts/generate-context.js",
    "lint:unused": "eslint . --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/native": "^7.1.8",
    "@stripe/stripe-react-native": "^0.57.2",
    "@supabase/supabase-js": "^2.84.0",
    "base64-arraybuffer": "^1.0.2",
    "dayjs": "^1.11.19",
    "expo": "~54.0.29",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.10",
    "expo-localization": "^17.0.8",
    "expo-router": "~6.0.19",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-task-manager": "~14.0.9",
    "i18next": "^25.7.3",
    "intl-pluralrules": "^2.0.1",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.0",
    "react-native": "0.81.5",
    "react-native-barcode-creator": "^0.1.8",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-ui-datepicker": "^3.1.2",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@expo/metro-runtime": "^6.1.2",
    "@react-native/typescript-config": "^0.82.1",
    "@types/node": "^25.0.3",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "autoprefixer": "^10.4.22",
    "dependency-cruiser": "^17.3.6",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.0.0",
    "jest": "~29.7.0",
    "knip": "^5.80.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-test-renderer": "19.1.0",
    "supabase": "^2.71.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}