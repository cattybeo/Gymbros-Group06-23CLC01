{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "constants/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "app.json",
    "babel.config.js",
    "metro.config.js"
  ],
  "project": ["**/*.{ts,tsx}"],
  "ignore": [
    "**/*.d.ts",
    "artifacts/**",
    "coverage/**"
  ],
  "ignoreDependencies": [
    "expo-router",
    "nativewind"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "classMembers": "warn",
    "duplicates": "warn",
    "enumMembers": "warn",
    "exports": "warn",
    "files": "warn",
    "nsExports": "warn",
    "nsTypes": "warn",
    "types": "warn",
    "unlisted": "warn",
    "unresolved": "warn"
  }
}
